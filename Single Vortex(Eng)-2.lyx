\begin_layout Standard
The Lagrangian density,
\begin_inset Formula 
\begin{equation}
\mathcal{L}=\mathcal{L}_{time}+\mathcal{L}_{kin}+\mathcal{L}_{pot}+\mathcal{L}_{int},
\end{equation}

\end_inset

has four components: temporal, kinetic, potential and interaction potential.
 Similarly we can split our Lagrangian,
\begin_inset Formula 
\begin{equation}
L=\int\mathcal{L}d\mathbf{r}.
\end{equation}

\end_inset

Following this way we have computed all parts of the Lagrangian.
\end_layout

\begin_layout Standard
Temporal term:
\begin_inset Formula 
\begin{align}
\mathcal{L}_{time}= & \frac{i\hbar}{2}\left[\psi^{*}\left(\mathbf{r},t\right)\frac{\partial\psi\left(\mathbf{r},t\right)}{\partial t}-\psi\left(\mathbf{r},t\right)\frac{\partial\psi^{*}\left(\mathbf{r},t\right)}{\partial t}\right]\nonumber \\
L_{time}= & -\frac{N\hbar}{2}\left(D_{1}\dot{B_{\rho}}R_{\rho}^{2}+D_{2}\dot{B_{z}}R_{z}^{2}+\frac{1}{2}D_{3}\dot{C}R_{\rho}^{4}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Kinetic term:
\begin_inset Formula 
\begin{align}
\mathcal{L}_{kin}= & -\frac{\hbar^{2}}{2m}\left[\nabla\psi^{*}\left(\mathbf{r},t\right)\right]\left[\nabla\psi\left(\mathbf{r},t\right)\right]\nonumber \\
L_{kin}= & -\frac{N\hbar^{2}}{2m}\left[D_{1}B_{\rho}^{2}R_{\rho}^{2}+D_{2}B_{z}^{2}R_{z}^{2}+2D_{3}B_{\rho}CR_{\rho}^{4}+R_{\rho}^{-2}\left(\ell^{2}D_{4}+D_{5}\right)+D_{6}C^{2}R_{\rho}^{6}\right].
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Potential term (harmonic trap):
\begin_inset Formula 
\begin{align}
\mathcal{L}_{pot}= & -\frac{1}{2}m\omega_{\rho}^{2}\left(\rho^{2}+\lambda^{2}z^{2}\right)\psi^{*}\left(\mathbf{r},t\right)\psi\left(\mathbf{r},t\right)\nonumber \\
L_{pot}= & -\frac{N}{2}m\omega_{\rho}^{2}\left(D_{1}R_{\rho}^{2}+\lambda^{2}D_{2}R_{z}^{2}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Atom-atom interaction potential term:
\begin_inset Formula 
\begin{align}
\mathcal{L}_{int}= & -\frac{g}{2}\left[\psi^{*}\left(\mathbf{r},t\right)\psi\left(\mathbf{r},t\right)\right]^{2}\nonumber \\
L_{int}= & -\frac{N^{2}gD_{7}}{2R_{\rho}^{2}R_{z}}.
\end{align}

\end_inset

The functions of 
\begin_inset Formula $\alpha$
\end_inset

, 
\begin_inset Formula $D_{i}$
\end_inset

, are: 
\begin_inset Formula $D_{i}=A_{i}/A_{0}$
\end_inset

, with exception of 
\begin_inset Formula $D_{7}=A_{7}/A_{0}^{2}$
\end_inset

.
 So the Lagrangian is the sum of each contribution, 
\begin_inset Formula $L=L_{time}+L_{kin}+L_{pot}+L_{int}$
\end_inset

.
 For simplicity we can scale the parameters of Lagrangian to make them dimension
less, which are the following changes:
\begin_inset Formula 
\begin{align*}
R_{\rho}(t)\rightarrow & a_{osc}r_{\rho}(t),\\
R_{z}(t)\rightarrow & a_{osc}r_{z}(t),\\
\xi(t)\rightarrow & a_{osc}r_{\xi}(t),\\
B_{\rho}(t)\rightarrow & a_{osc}^{-2}\beta_{\rho}(t),\\
B_{z}(t)\rightarrow & a_{osc}^{-2}\beta_{z}(t),\\
C(t)\rightarrow & a_{osc}^{-4}\zeta(t)
\end{align*}

\end_inset

and
\begin_inset Formula 
\[
t\rightarrow\omega_{\rho}^{-1}\tau;
\]

\end_inset

where the harmonic oscillator length is 
\begin_inset Formula $a_{osc}=\sqrt{\hbar/m\omega_{\rho}}$
\end_inset

, and the dimensionless parameter of interaction is 
\begin_inset Formula $\gamma=Na_{s}/a_{osc}$
\end_inset

.
 Thus the Lagrangian becomes
\begin_inset Formula 
\begin{align}
L= & -\frac{N\hbar\omega_{\rho}}{2}\left[D_{1}r_{\rho}^{2}\left(\dot{\beta_{\rho}}+\beta_{\rho}^{2}+1\right)+D_{2}r_{z}^{2}\left(\dot{\beta_{z}}+\beta_{z}^{2}+\lambda^{2}\right)\right.\nonumber \\
 & \left.+D_{3}r_{\rho}^{4}\left(\frac{1}{2}\dot{\zeta}+2\beta_{\rho}\zeta\right)+\ell^{2}r_{\rho}^{-2}\left(D_{4}+D_{5}\right)+D_{6}\zeta^{2}r_{\rho}^{6}+D_{7}\frac{4\pi\gamma}{r_{\rho}^{2}r_{z}}\right].\label{eq:Lagrangian}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Taking the Euler-Lagrange equations,
\begin_inset Formula 
\begin{align}
\frac{d}{dt}\left(\frac{\partial L}{\partial\dot{q_{i}}}\right)-\frac{\partial L}{\partial q_{i}}= & 0,
\end{align}

\end_inset

for each one of the six variational parameters from Lagrangian 
\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:Lagrangian"

\end_inset

 hence six differential equations:
\begin_inset Formula 
\begin{align}
\beta_{\rho}-\frac{\dot{r_{\rho}}}{r_{\rho}}-\frac{D_{1}^{\prime}\dot{\alpha}}{2D_{1}}+\frac{D_{3}r_{\rho}^{2}\zeta}{D_{1}} & =0\label{eq:1a}\\
\beta_{z}-\frac{\dot{r_{z}}}{r_{z}}-\frac{D_{2}^{\prime}\dot{\alpha}}{2D_{2}} & =0\label{eq:1b}\\
\zeta-\frac{D_{3}\dot{r_{\rho}}}{D_{6}r_{\rho}}-\frac{D_{3}\dot{\alpha}}{4D_{6}r_{\rho}^{2}}+\frac{D_{3}\beta_{\rho}}{D_{6}r_{\rho}^{2}} & =0\label{eq:1c}\\
D_{1}r_{\rho}\left(\dot{\beta_{\rho}}+\beta_{\rho}^{2}+1\right)+D_{3}r_{\rho}^{3}\left(\dot{\zeta}+4\beta_{\rho}\zeta\right)-\nonumber \\
\frac{\ell^{2}}{r_{\rho}^{3}}\left(D_{4}+D_{5}\right)+3D_{6}\zeta^{2}r_{\rho}^{5}-D_{7}\frac{4\pi\gamma}{r_{\rho}^{3}r_{z}} & =0\label{eq:1d}\\
D_{2}r_{z}\left(\dot{\beta_{z}}+\beta_{z}^{2}+\lambda^{2}\right)-D_{7}\frac{2\pi\gamma}{r_{\rho}^{2}r_{z}^{2}} & =0\label{eq:1e}\\
D_{1}^{\prime}r_{\rho}^{2}\left(\dot{\beta_{\rho}}+\beta_{\rho}^{2}+1\right)+D_{2}^{\prime}r_{z}^{2}\left(\dot{\beta_{z}}+\beta_{z}^{2}+\lambda^{2}\right)+D_{3}^{\prime}r_{\rho}^{4}\left(\frac{1}{2}\dot{\zeta}+2\beta_{\rho}\zeta\right)+\nonumber \\
\frac{\ell^{2}}{r_{\rho}^{2}}\left(D_{4}^{\prime}+D_{5}^{\prime}\right)+D_{6}^{\prime}\zeta^{2}r_{\rho}^{6}-D_{7}^{\prime}\frac{4\pi\gamma}{r_{\rho}^{2}r_{z}} & =0.\label{eq:1f}
\end{align}

\end_inset

 
